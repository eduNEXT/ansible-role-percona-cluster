# Ansible managed

# This does not change any existing databases or rows, only the defaults for newly created databases

[client]
default-character-set={{ percona_default_charset }}

[mysql]
default-character-set={{ percona_default_charset }}


[mysqld]
collation-server = {{ percona_default_collation }}
init-connect='SET NAMES {{ percona_default_charset }}'
character-set-server = {{ percona_default_charset }}
